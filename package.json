{
    "name": "cisco",
    "displayName": "Cisco IOS Syntax",
    "description": "Cisco IOS Syntax Highlighter",
    "version": "0.0.6",
    "publisher": "jamiewoodio",
    "icon": "images/cisco.jpg",
    "engines": {
        "vscode": "^1.5.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/woodjamie/vscode-cisco-syntax.git"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "cisco",
            "aliases": ["Cisco", "cisco"],
            "extensions": [".ios", ".cfg", ".txt"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "cisco",
            "scopeName": "text.cisco",
            "path": "./syntaxes/cisco.tmLanguage"
        }]
    }
}