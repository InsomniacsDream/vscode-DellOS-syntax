{
    "name": "cisco",
    "displayName": "Cisco IOS Syntax",
    "description": "Cisco IOS Syntax Highlighter",
    "version": "0.0.1",
    "publisher": "jamiewoodio",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
